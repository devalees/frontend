{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Frontend project with test-driven development",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:track": "node scripts/update-coverage-report.js",
    "test:track:api": "node scripts/track-coverage.js api",
    "test:track:module": "node scripts/track-coverage.js",
    "test:track:global": "node scripts/track-coverage.js --global",
    "test:track:manual": "node scripts/track-coverage.js --update --module",
    "test:migrate": "node scripts/migrate-tests.js",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "test:vitest": "vitest run",
    "test:vitest:watch": "vitest",
    "test:vitest:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
